{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-var-requires": false,
        "no-internal-module": true,
        "object-literal-sort-keys": false,
        "member-access": false,
        "no-submodule-imports": false,
        "no-empty": false,
        "no-console": false
    },
    "defaultSeverity": "warning"
}